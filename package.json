{
  "name": "dashboard",
  "displayName": "dashboard",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "Testing Extension",
          "icon": "assets/test-data-svgrepo-com.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "dashboard.openview",
          "name": "Dashboard",
          "contextualTitle": "View"
        }
      ]
    },
    "commands": [
      {
        "command": "dashboard.openview",
        "title": "Sidebar View"
      },
      {
        "command": "dashboard.menu.view",
        "category": "vscode-extension-sidebar-html",
        "title": "Sample WebView in VS Code Sidebar",
        "icon": "$(clear-all)"
      },
      {
        "command": "vscode-run-tests.runTests",
        "title": "Run Tests"
      },
      {
        "command": "vscode-run-tests.updateResults",
        "title": "Update Results"
      },
      {
        "command": "vscode-run-tests.getCoverage",
        "title": "Get Coverage"
      },
      {
        "command": "vscode-run-tests.updateCoverage",
        "title": "Update Coverage Results"
      },
      {
        "command": "vscode-slowest-tests.slowestTests",
        "title": "Find Slowest Tests"
      },
      {
        "command": "vscode-slowest-tests.updateSlowestTests",
        "title": "Update Slowest Tests Results"
      },
      {
        "command": "code-tutor.annotate",
        "title": "Toggle Tutor Annotations"
      },
      {
        "command": "fix-failing-tests.fixFailingTests",
        "title": "Fix Failing Tests"
      },
      {
        "command": "fix-coverage.fixCoverage",
        "title": "Fix Coverage"
      },
      {
        "command": "optimise-slowest.optimiseSlowest",
        "title": "Optimise Slowest Tests"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dashboard.menu.view",
          "group": "navigation",
          "when": "view == dashboard.openview"
        }
      ],
      "editor/title": [
        {
          "command": "code-tutor.annotate",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  }
}