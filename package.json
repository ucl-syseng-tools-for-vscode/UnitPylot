{
  "name": "unitpylot",
  "displayName": "UnitPylot",
  "icon": "assets/unitpylot-icon-colour.png",
  "description": "Pytest assistant with AI-powered test insights",
  "publisher": "unitpylot",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Testing",
    "AI",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "UnitPylot",
          "icon": "assets/unitpylot-icon-black.png"
        }
      ]
    },
    "chatParticipants": [
      {
        "id": "unitpylot-chat",
        "name": "UnitPylot-AAA-Assistant",
        "description": "Implement good python testing practices using the AAA pattern"
      }
    ],
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "unitpylot.webview",
          "name": "Dashboard",
          "contextualTitle": "View",
          "icon": "assets/unitpylot-icon-black.png"
        },
        {
          "type": "tree",
          "id": "unitpylot.failingtreeview",
          "name": "Tests Overview",
          "contextualTitle": "Tests Overview",
          "icon": "assets/unitpylot-icon-black.png"
        },
        {
          "type": "tree",
          "id": "unitpylot.graphdoctreeview",
          "name": "Graphs & Docs",
          "contextualTitle": "Graphs and Docs",
          "icon": "assets/unitpylot-icon-black.png"
        }
      ]
    },
    "commands": [
      {
        "command": "unitpylot.vscode-run-tests.runTests",
        "title": "UnitPylot: Run Tests"
      },
      {
        "command": "unitpylot.vscode-run-tests.runAllTests",
        "title": "UnitPylot: Run All Tests"
      },
      {
        "command": "unitpylot.vscode-run-tests.updateResults",
        "title": "UnitPylot: Update Results"
      },
      {
        "command": "unitpylot.vscode-run-tests.getCoverage",
        "title": "UnitPylot: Get Coverage"
      },
      {
        "command": "unitpylot.vscode-run-tests.updateCoverage",
        "title": "UnitPylot: Update Coverage Results"
      },
      {
        "command": "unitpylot.vscode-slowest-tests.slowestTests",
        "title": "UnitPylot: Find Slowest Tests"
      },
      {
        "command": "unitpylot.vscode-slowest-tests.updateSlowestTests",
        "title": "UnitPylot: Update Slowest Tests Results"
      },
      {
        "command": "unitpylot.vscode-run-tests.updateMemory",
        "title": "UnitPylot: Update Memory Results"
      },
      {
        "command": "unitpylot.code-tutor.annotate",
        "title": "UnitPylot: Test Insights"
      },
      {
        "command": "unitpylot.fix-failing-tests.fixFailingTests",
        "title": "UnitPylot: Fix Failing Tests"
      },
      {
        "command": "unitpylot.fix-coverage.fixCoverage",
        "title": "UnitPylot: Fix Coverage"
      },
      {
        "command": "unitpylot.optimise-slowest.optimiseSlowest",
        "title": "UnitPylot: Optimise Slowest Tests"
      },
      {
        "command": "unitpylot.generate-pydoc.generatePydoc",
        "title": "UnitPylot: Generate Pydoc"
      },
      {
        "command": "unitpylot.acceptSuggestion",
        "title": "UnitPylot: Accept Annotation onto corresponding test file for coverage"
      },
      {
        "command": "unitpylot.rejectSuggestion",
        "title": "UnitPylot: Reject Annotation"
      },
      {
        "command": "unitpylot.addSuggestiontoSameFile",
        "title": "UnitPylot: Add Annotation to Same File"
      },
      {
        "command": "unitpylot.addSuggestiontoMainFile",
        "title": "UnitPylot: Add Annotation to corresponding Main File"
      },
      {
        "command": "unitpylot.test-history.showPassFailGraph",
        "title": "UnitPylot: Show test pass-fail history graph"
      },
      {
        "command": "unitpylot.test-history.showCoverageGraph",
        "title": "UnitPylot: Show coverage history graph"
      },
      {
        "command": "unitpylot.openSettings",
        "title": "Open Settings",
        "icon": "$(gear)"
      },
      {
        "command": "unitpylot.optimise-memory.optimiseMemory",
        "title": "UnitPylot: Optimise Memory Usage of Tests"
      },
      {
        "command": "unitpylot.failingtests.refreshView",
        "title": "UnitPylot: Refresh Failing Tests"
      },
      {
        "command": "unitpylot.openTestFile",
        "title": "Open Test File"
      },
      {
        "command": "unitpylot.runTestsInFile",
        "title": "UnitPylot: Run Tests in File"
      },
      {
        "command": "unitpylot.runSpecificTest",
        "title": "UnitPylot: Run Specific Test"
      },
      {
        "command": "unitpylot.exportSnapshotReport",
        "title": "UnitPylot: Download Snapshot Report"
      },
      {
        "command": "unitpylot.updateSidebar",
        "title": "UnitPylot: Update Sidebar"
      },
      {
        "command": "unitpylot.failingTestsProvider.showInfo",
        "title": "Show Info",
        "icon": "$(info)"
      },
      {
        "command": "unitpylot.graphsDocsProvider.showInfo",
        "title": "Show Info",
        "icon": "$(info)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "unitpylot.openSettings",
          "when": "view == unitpylot.webview",
          "group": "navigation"
        },
        {
          "command": "unitpylot.failingTestsProvider.showInfo",
          "when": "view == unitpylot.failingtreeview",
          "group": "navigation",
          "tooltip": "This tree view displays failing tests, slow tests, and memory-intensive tests.\n\nIcons:\n- Red Cross: Indicates a failing test\n- Stopwatch: Indicates a slow test\n- RAM: Indicates a memory-intensive test\n- Green Tick: Indicates a passing test file."
        },
        {
          "command": "unitpylot.graphsDocsProvider.showInfo",
          "when": "view == unitpylot.graphdoctreeview",
          "group": "navigation",
          "tooltip": "This tree view displays graphs and documentation related to the tests.\n\nIcons:\n- Graph: Indicates a graph\n- Doc: Indicates a documentation file."
        }
      ],
      "editor/title": [
        {
          "command": "unitpylot.code-tutor.annotate",
          "group": "navigation"
        },
        {
          "command": "unitpylot.runTestsInFile",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "group": "llm-commands",
          "submenu": "unitpylot.commands"
        }
      ],
      "unitpylot.commands": [
        {
          "command": "unitpylot.fix-failing-tests.fixFailingTests",
          "group": "llm-commands",
          "when": "resourceLangId == python && (resourceFilename =~ /test_.*/ || resourceFilename =~ /.*_test.py/)"
        },
        {
          "command": "unitpylot.fix-coverage.fixCoverage",
          "group": "llm-commands",
          "when": "resourceLangId == 'python' && !(resourceFilename =~ /^test_/) && !(resourceFilename =~ /_test\\.py$/)"
        },
        {
          "command": "unitpylot.optimise-slowest.optimiseSlowest",
          "group": "llm-commands",
          "when": "resourceLangId == python && (resourceFilename =~ /test_.*/ || resourceFilename =~ /.*_test.py/)"
        },
        {
          "command": "unitpylot.generate-pydoc.generatePydoc",
          "group": "llm-commands",
          "when": "resourceLangId == python && (resourceFilename =~ /test_.*/ || resourceFilename =~ /.*_test.py/)"
        },
        {
          "command": "unitpylot.optimise-memory.optimiseMemory",
          "group": "llm-commands",
          "when": "resourceLangId == python && (resourceFilename =~ /test_.*/ || resourceFilename =~ /.*_test.py/)"
        }
      ],
      "commandPalette": [
        {
          "command": "unitpylot.vscode-run-tests.runTests",
          "title": "UnitPylot: Run Tests"
        },
        {
          "command": "unitpylot.vscode-run-tests.runAllTests",
          "title": "UnitPylot: Run All Tests"
        },
        {
          "command": "unitpylot.vscode-run-tests.updateResults",
          "title": "UnitPylot: Update Results",
          "when": "false"
        },
        {
          "command": "unitpylot.vscode-run-tests.getCoverage",
          "title": "UnitPylot: Get Coverage",
          "when": "false"
        },
        {
          "command": "unitpylot.vscode-run-tests.updateCoverage",
          "title": "UnitPylot: Update Coverage Results",
          "when": "false"
        },
        {
          "command": "unitpylot.vscode-slowest-tests.slowestTests",
          "title": "UnitPylot: Find Slowest Tests",
          "when": "false"
        },
        {
          "command": "unitpylot.vscode-slowest-tests.updateSlowestTests",
          "title": "UnitPylot: Update Slowest Tests Results",
          "when": "false"
        },
        {
          "command": "unitpylot.vscode-run-tests.updateMemory",
          "title": "UnitPylot: Update Memory Results",
          "when": "false"
        },
        {
          "command": "unitpylot.code-tutor.annotate",
          "title": "UnitPylot: Test Insights"
        },
        {
          "command": "unitpylot.fix-failing-tests.fixFailingTests",
          "title": "UnitPylot: Fix Failing Tests"
        },
        {
          "command": "unitpylot.fix-coverage.fixCoverage",
          "title": "UnitPylot: Fix Coverage"
        },
        {
          "command": "unitpylot.optimise-slowest.optimiseSlowest",
          "title": "UnitPylot: Optimise Slowest Tests"
        },
        {
          "command": "unitpylot.generate-pydoc.generatePydoc",
          "title": "UnitPylot: Generate Pydoc"
        },
        {
          "command": "unitpylot.acceptSuggestion",
          "title": "UnitPylot: Accept Annotation onto corresponding test file for coverage",
          "when": "false"
        },
        {
          "command": "unitpylot.rejectSuggestion",
          "title": "UnitPylot: Reject Annotation",
          "when": "false"
        },
        {
          "command": "unitpylot.addSuggestiontoSameFile",
          "title": "UnitPylot: Add Annotation to Same File",
          "when": "false"
        },
        {
          "command": "unitpylot.addSuggestiontoMainFile",
          "title": "UnitPylot: Add Annotation to corresponding Main File",
          "when": "false"
        },
        {
          "command": "unitpylot.test-history.showPassFailGraph",
          "title": "UnitPylot: Show test pass-fail history graph",
          "when": "false"
        },
        {
          "command": "unitpylot.test-history.showCoverageGraph",
          "title": "UnitPylot: Show coverage history graph"
        },
        {
          "command": "unitpylot.openSettings",
          "title": "Open Settings",
          "icon": "$(gear)"
        },
        {
          "command": "unitpylot.optimise-memory.optimiseMemory",
          "title": "UnitPylot: Optimise Memory Usage of Tests"
        },
        {
          "command": "unitpylot.failingtests.refreshView",
          "title": "UnitPylot: Refresh Failing Tests"
        },
        {
          "command": "unitpylot.openTestFile",
          "title": "Open Test File",
          "when": "false"
        },
        {
          "command": "unitpylot.runTestsInFile",
          "title": "UnitPylot: Run Tests in File",
          "when": "false"
        },
        {
          "command": "unitpylot.runSpecificTest",
          "title": "UnitPylot: Run Specific Test",
          "when": "false"
        },
        {
          "command": "unitpylot.exportSnapshotReport",
          "title": "UnitPylot: Download Snapshot Report"
        },
        {
          "command": "unitpylot.updateSidebar",
          "title": "UnitPylot: Update Sidebar",
          "when": "false"
        },
        {
          "command": "unitpylot.failingTestsProvider.showInfo",
          "title": "Show Info",
          "icon": "$(info)",
          "when": "flase"
        },
        {
          "command": "unitpylot.graphsDocsProvider.showInfo",
          "title": "Show Info",
          "icon": "$(info)",
          "when": "false"
        }
      ]
    },
    "submenus": [
      {
        "id": "unitpylot.commands",
        "label": "UnitPylot Commands",
        "group": "llm-commands"
      }
    ],
    "configuration": {
      "title": "UnitPylot",
      "properties": {
        "unit-pylot.runNecessaryTestsOnly": {
          "type": "boolean",
          "default": true,
          "description": "Run only necessary tests instead of all tests."
        },
        "unit-pylot.copilotEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable GitHub Copilot integration."
        },
        "unit-pylot.codeCoverageHighlighting": {
          "type": "boolean",
          "default": true,
          "description": "Enable code coverage highlighting."
        },
        "unit-pylot.numberOfSlowestTests": {
          "type": "number",
          "default": 3,
          "description": "Number of slowest tests to display."
        },
        "unit-pylot.numberOfMemoryIntensiveTests": {
          "type": "number",
          "default": 3,
          "description": "Number of memory intensive tests to display."
        },
        "unit-pylot.snapshotInterval": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "description": "Snapshot interval in minutes. (Extension restart required)"
        },
        "unit-pylot.runTestsOnSave": {
          "type": "boolean",
          "default": false,
          "description": "Run tests on any file save."
        },
        "unit-pylot.runTestsInBackground": {
          "type": "boolean",
          "default": true,
          "description": "Periodically run tests in the background."
        },
        "unit-pylot.saveSnapshotOnTestRun": {
          "type": "boolean",
          "default": true,
          "description": "Save snapshots when tests are run."
        },
        "unit-pylot.saveSnapshotPeriodically": {
          "type": "boolean",
          "default": false,
          "description": "Periodically save snapshots in the background."
        },
        "unit-pylot.runTestsInterval": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "description": "Background test run interval in minutes. (Extension restart required)"
        },
        "unit-pylot.customLLM Endpoint": {
          "type": "string",
          "default": "http://localhost:8000/v1/chat/completions",
          "description": "Custom LLM endpoint to use if GitHub Copilot is not enabled.\nCompatible with OpenAI-compatible APIs. \nSome examples are: OpenAI, vLLM, Ollama, etc."
        },
        "unit-pylot.customLLM Model": {
          "type": "string",
          "default": "",
          "description": "Custom LLM model to use if GitHub Copilot is not enabled."
        },
        "unit-pylot.customLLM API Key": {
          "type": "string",
          "default": "",
          "description": "Custom LLM key to use if GitHub Copilot is not enabled. (Optional)"
        },
        "unit-pylot.customLLM MaxTokens": {
          "type": "number",
          "default": 500,
          "description": "Custom LLM max tokens to use if GitHub Copilot is not enabled."
        }
      }
    }
  },
  "scripts": {
    "clean": "rm -rf out",
    "vscode:prepublish": "npm run compile && npm run copy-files",
    "compile": "tsc -p ./ && npm run copy-files",
    "watch": "npm run copy-files && tsc -watch -p ./",
    "pretest": "npm run compile && npm run copy-files && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "copy-files": "copyfiles -u 1 \"src/**/*.py\" \"src/**/*.html\" out/"
  },
  "devDependencies": {
    "@types/chai": "^5.2.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/sinon": "^17.0.4",
    "@types/sqlite3": "^3.1.11",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^1.6.2",
    "chai": "4.5",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.2.2",
    "sinon": "^19.0.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "chartjs-plugin-doughnutlabel": "^2.0.3",
    "sqlite3": "^5.1.7"
  }
}